<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <context id="MSTables" targetRuntime="MyBatis3" defaultModelType="flat">
        <!--<properties resource="application.properties"/>-->

        <property name="autoDelimitKeywords" value="true"/>
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <property name="javaFileEncoding" value="utf-8"/>

        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>

        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!-- 数据库 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/kxl_message?useSSL=false"
                        userId="root"
                        password="123456">
        </jdbcConnection>

        <!--指定生成的类型为java类型，避免数据库中number等类型字段 -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>



        <!--实体存放路径 -->
        <javaModelGenerator targetPackage="com.kingxunlian.message.biz.dto" targetProject="src/main/java">
            <property name="MessageLog" value="true"/>
        </javaModelGenerator>
        <!-- 自动生成的Mapper.xml的存放路径 -->
        <sqlMapGenerator targetPackage="com.kingxunlian.message.biz.mapper" targetProject="src/main/java">
            <property name="MessageLogMapper" value="true"/>
        </sqlMapGenerator>
        <!-- 自动生成的Mapper.java 的存放路径-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.kingxunlian.message.biz.mapper" targetProject="src/main/java">
            <property name="MessageLogMapper" value="true"/>
        </javaClientGenerator>
        <!-- 对应的数据库的表 -->
        <table schema="" tableName="kxl_message_log" domainObjectName="MessageLog"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">
        </table>
    </context>
</generatorConfiguration>
